{
  "name": "http-francis",
  "version": "0.1.6",
  "description": "",
  "main": "dist/http-francis.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/D-Mobilelab/http-francis.git"
  },
  "scripts": {
    "build": "npm run clean && npm run build:dev && npm run build:prd",
    "build:dev": "webpack dist/$npm_package_name.js --config tools/webpack.config.dev.js",
    "build:prd": "webpack dist/$npm_package_name.min.js --config tools/webpack.config.prd.js",
    "prepublish": "npm run test && npm run documentation && npm run build",
    "clean": "rimraf dist build docs",
    "lint": "eslint src",
    "preversion": "npm run test",
    "version": "npm run build && npm run gen:docs && git add dist docs -f",
    "test": "karma start --single-run --no-auto-watch karma.config.js",
    "test:watch": "karma start karma.config.js",
    "documentation": "npm run gen:docs && npm run gen:readme",
    "serve:docs": "./node_modules/documentation/bin/documentation.js serve src/main.js",
    "gen:docs": "./node_modules/documentation/bin/documentation.js build src/main.js -f html -o docs/$npm_package_version",
    "gen:readme": "./node_modules/documentation/bin/documentation.js readme src/main.js --section=API"
  },
  "keywords": [
    "es6",
    "babel",
    "webpack",
    "jasmine",
    "karma",
    "istanbul",
    "http",
    "client"
  ],
  "author": "Pasquale Mangialavori",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-eslint": "^9.0.0",
    "babel-istanbul": "^0.12.2",
    "babel-istanbul-loader": "0.1.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.3.13",
    "documentation": "^8.1.2",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "gh-pages": "^2.0.0",
    "jasmine-ajax": "^3.3.1",
    "jasmine-core": "^3.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-coveralls": "^2.0.0",
    "karma-jasmine": "^1.1.2",
    "karma-mocha-reporter": "^2.2.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.5",
    "loader-utils": "^1.1.0",
    "rimraf": "^2.5.0",
    "webpack": "^4.19.1"
  },
  "dependencies": {
    "babel-runtime": "^6.20.0",
    "core-decorators": "^0.20.0",
    "docomo-utils": "https://github.com/D-Mobilelab/docomo-utils/tarball/v0.1.0"
  }
}
